<!DOCTYPE html>
<html>
  <head>
    <title> Title </title>
    <%= stylesheet_link_tag    "application" %>
    <%= stylesheet_link_tag    "print", :media => "print" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <div id="content">
      <% flash.each do |name, msg| %>
	<%= content_tag :div, msg, :id => "flash_#{name}" %>
      <% end %>
      <%= yield %>
    </div>
  </body>
</html>
